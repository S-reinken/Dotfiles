ames slideOutText1lb { 0% {left: 50%;} 100% {left: -50%;} } @-webkit-keyframes slideOutText1lb { 0% {left: 50%;} 100% {left: -50%;} } @keyframes slideOutText1lb { 0% {left: 50%;} 100% {left: -50%;} } @-webkit-keyframes slideInText2lb { 0% {left: -50%;} 100% {left: 0%;} } @-webkit-keyframes shiftDisclaimerLlb { 0% {background-position: 20px 0px} 100% {background-position: 0px 0px} } @keyframes shiftDisclaimerLlb { 0% {background-position: 20px 0px} 100% {background-position: 0px 0px} } @-webkit-keyframes slideInDisclaimer1lb { 0% {left: -50%;} 100% {left: 53%;} } @keyframes slideInDisclaimer1lb { 0% {left: -50%;} 100% {left: 53%;} } @-webkit-keyframes slideOutDisclaimer1lb { 0% {left: 53%;} 100% {left: 50%;} } @keyframes slideOutDisclaimer1lb { 0% {left: 53%;} 100% {left: 50%;} } @-webkit-keyframes slideBackDisclaimer1lb { 0% {left: 50%;} 100% {left: 53%;} } @keyframes slideBackDisclaimer1lb { 0% {left: 50%;} 100% {left: 53%;} } @-webkit-keyframes slideInTextws { 0% {left: -100%;} 100% {left: 5px;} } @keyframes slideInTextws { 0% {left: -100%;} 100% {left: 5px;} } @-webkit-keyframes slideOutTextws { 0% {left: 5px;} 100% {left: 100%;} } @keyframes slideOutTextws { 0% {left: 5px;} 100% {left: 100%;} } @-webkit-keyframes slideInPanelws { 0% {left: -215%;} 100% {left: -36%;} } @keyframes slideInPanelws { 0% {left: -215%;} 100% {left: -36%;} } @-webkit-keyframes slideOutPanelws { 0% {left: -36%;} 100% {left: 133%;} } @keyframes slideOutPanelws { 0% {left: -36%;} 100% {left: 133%;} } @-webkit-keyframes slideInCTAsws { 0% {left: -100%;} 100% {left: 0%;} } @keyframes slideInCTAsws { 0% {left: -100%;} 100% {left: 0%;} } @-webkit-keyframes slideInOfferContainerws { 0% {left: -100%; opacity: 0;} 100% {left: 0%;opacity: 1;} } @keyframes slideInOfferContainerws { 0% {left: -100%; opacity: 0;} 100% {left: 0%;opacity: 1;} } @-webkit-keyframes slideOutOfferContainerws { 0% {left: 0%; opacity: 1;} 100% {left: 133%; opacity: 0;} } @keyframes slideOutOfferContainerws { 0% {left: 0%; op