document.getElementById('adm_htmlad_container').innerHTML = "<div id=\"template\" class=\"loading lb\"> <div id=\"img_lifestyle_lb\" class=\"lifestyleart\"></div> <div id=\"img_offer_lsleaderboard\" class=\"lifestyleart\"></div> <div id=\"slidebackground\"></div> <div id=\"contentsliderlb\"> <div id=\"offer_container\"> <div id=\"float_container\"> <div id=\"img_offer_leaderboard\" class=\"nodrag\"></div> <div id=\"offer_subtitle\" class=\"offertext\"></div> </div> </div> <div id=\"txt_nameplate\" ifont = \"size: 9; linespacing: -2\"></div> <div id=\"txt_preroll\" ifont = \"location: northwest\"></div> <div id=\"txt_title\"></div> <div id=\"txt_tagline\"></div> <div id=\"legal_button\"> <div id=\"txt_disclaimer\"></div> </div> </div> <div id=\"offer_legal_container\"> <div id=\"offer_legal\"></div> <div id=\"close_button\">&#x2716; Close</div> </div> <div id=\"logoOne\" class=\"logoblock_lb\"> <div id=\"_img_logobackground_lb\" class=\"nodrag\"></div> <div id=\"_img_bowtie\" class=\"nodrag\"></div> <div id=\"_img_chevylogotype\"></div> </div> <div id=\"img_logoTwo_lb\" class=\"logoblock_lb\"></div> <div id=\"ctas\"> <div id=\"cta2\" class=\"cta2\"></div> <div id=\"cta\" class=\"cta\"></div> </div> <div id=\"txt_link\"></div> <div id=\"overlay\"></div> </div>";

adm.cs.fallbacklifestyle = function() {
var userImage = ("img_lifestyle_" + adm.cs.adSize);
	if (!adm.ad.param(userImage)) {
		adm.ad.param(userImage, "support/" + adm.cs.adSize +"_fallback.jpg")
	}
};


adm.cs.LifestyleChooser = function() {
	var res = adm.cs.imageName.slice(10);
	var handle = ("ls" + res);
	var slots = adm.ad.param('offer_imageslots');
	var offerImage = ("img_offer_ls" + res);
	var userImage = ("img_lifestyle_" + adm.cs.adSize);
	if (adm.ad.param(userImage) == "") {
		adm.ad.param(userImage, "support/lb_fallback.jpg")
	}
	if(slots && slots.match(handle)) {
		adm.$(userImage).hide();
		adm.$(offerImage).show();
	} else {
		adm.$(offerImage).hide();
		adm.$(userImage).show();
	};
};

adm.cs.applyNameplate = function() {
	
	if (adm.ad.param("txt_nameplate")) {
		adm.ad.param("_txt_nameplate_proxy", adm.ad.param("txt_nameplate"));
	} else if (adm.ad.param("offer_vehiclenameplate")) {
		adm.ad.param("txt_nameplate", adm.ad.param("offer_vehiclenameplate"));
		adm.ad.param("_txt_nameplate_proxy", adm.ad.param("offer_vehiclenameplate"));
	} else {
		adm.$("txt_nameplate").hide();
		if (adm.cs.adSize == "ss") {
			adm.$("_txt_nameplate_proxy").hide();
		}
	}
}


adm.cs.showMainOrImage = function() {
	var res = adm.cs.imageName.slice(10);
	var handle = ("\\b" + res + "\\b");
	var slots = adm.ad.param('offer_imageslots');
	if(slots && slots.match(handle) || adm.cs.offerStyle === "Chevyoffer") {
		unused = ['offer_subtitle', 'offer_exclusions', 'offer_additionalterms', 'offer_startdate', 'offer_enddate' ];
		adm.$(adm.cs.imageName).show();
	} else {
		unused = ['offer_exclusions', 'offer_additionalterms', 'offer_startdate', 'offer_enddate' ];
		adm.$(adm.cs.imageName).hide();
	}
	for(var i = 0; i < unused.length; i++) {
		adm.$(unused[i]).hide();
	}
};


adm.cs.setDisclaimerCTA = function(str) {
    return str.split(/\s+/).slice(0,3).join(" ");
}

adm.ad.on( "init", function() {
	
	
	
	adm.ad.defaultStyles = {}; 
	
	
	if(!adm.cs) { adm.cs = { }; }
	adm.cs.isFallback = false;
	adm.cs.isT3 = false;
	adm.cs.isT2 = false;
	adm.cs.isValidZip = false;
	adm.cs.isStatic = false;
	

	

	if(adm.$('template').hasClass('mr')) {
		adm.cs.adSize = 'mr';
	} else if(adm.$('template').hasClass('lb')) {
		adm.cs.adSize = 'lb';
	} else if(adm.$('template').hasClass('ws')) {
		adm.cs.adSize = 'ws';
	} else if(adm.$('template').hasClass('ss')) {
		adm.cs.adSize = 'ss';
	} else if(adm.$('template').hasClass('ct')) {
		adm.cs.adSize = 'ct';
	} else if(adm.$('template').hasClass('sw')) {
		adm.cs.adSize = 'sw';
	} else if(adm.$('template').hasClass('sn')) {
		adm.cs.adSize = 'sn';
	}


	switch(adm.cs.adSize) {
		case 'mr':
			adm.cs.imageName = 'img_offer_mediumrectangle';
			break;
		case 'lb':
			adm.cs.imageName = 'img_offer_leaderboard';
			break;
		case 'ws':
			adm.cs.imageName = 'img_offer_wideskyscraper';
			break;
		case 'ss':
			adm.cs.imageName = 'img_offer_smallskyscraper';
			break;
		case 'ct':
			adm.cs.imageName = 'img_offer_catfishtall';
			break;
		case 'sn':
			adm.cs.imageName = 'img_offer_smartphonenarrow';
			break;
		case 'sw':
			adm.cs.imageName = 'img_offer_smartphonewide';
			break;
	}
	adm.cs.divOrder = [];

} );

adm.ad.on( "dealer_fallback", function() {
	
	adm.ad.param("txt_preroll", "Special offers are available from your local dealer");
	adm.ad.param("txt_title", "Visit Your Local Dealer");
	adm.ad.param("txt_link", "Find a Dealer");
	adm.ad.param("txt_tagline", "To learn more about special offers");	
	adm.$("cta2").hide();
	adm.$('cta').innerHTML = "Find a Dealer";
	adm.$("template").replaceClass("ready", "static");
	adm.cs.isStatic = true;
	
	adm.cs.isFallback = true;
} );

adm.ad.on( "pre_image", function() {
	
	
	if (adm.ad.param("t3search")) {
		adm.cs.isT3 = true;
	} else if(adm.ad.param("contactbrand")) {
		adm.cs.isT2 = true;
		if(adm.ad.param("dealerzip")) {
			adm.cs.isValidZip = true;
		}
	};
	
	
	if (adm.ad.lastContact && !adm.ad.param("txt_tagline")) {
		adm.ad.param("txt_tagline", adm.ad.param("txt_city") + ", " + adm.ad.param("txt_state"));
	}

	
	adm.cs.applyNameplate();
	
	adm.cs.fallbacklifestyle();
	if(adm.cs.adSize == "sn" || adm.cs.adSize == "sw" || adm.cs.adSize == "ct") {
		return;
	};
	
	
	
	if (adm.ad.param("offer_legal")){
		adm.ad.param("txt_disclaimer", adm.cs.setDisclaimerCTA(adm.ad.param("offer_legal")));
	}
	
} );

adm.ad.on("pre_image", function() {

	
	adm.$("#offer_container").style.backgroundColor = adm.htmlad.color(0);
	if(adm.cs.adSize == "lb") {
		adm.$("#slidebackground").style.backgroundColor = adm.htmlad.color(0);
		adm.$("#slidebackground").style.borderColor = adm.htmlad.color(0);
	} else {
		if (adm.cs.adSize != "sn" && adm.cs.adSize != "sw"  && adm.cs.adSize != "ct") {
			adm.$("#footer").style.backgroundColor = adm.htmlad.color(2);
		}
	};
	
	
	var additionalLegal = adm.ad.param("txt_additional_legal");
	if(additionalLegal) {
		if(additionalLegal != "" && additionalLegal != "Insert Additional Legal Text Here") {
			adm.ad.param("offer_legal", adm.ad.param("offer_legal") + "<br/>" + additionalLegal);
		}
	};
	
	
	
	if(adm.cs.adSize == 'sw' || adm.cs.adSize == 'sn' || adm.cs.adSize == 'ct' || adm.cs.isFallback) {
		return;
	} else {
		
		arr = ["txt_preroll", "txt_title", "txt_tagline", "ctas", "cta2", "slidebackground", "offer_container"];
		if(adm.cs.adSize == 'lb') {
			arr.push("contentsliderlb", "legal_button")
		};
		if(adm.cs.adSize == 'ss') {
			arr.push("txt_nameplate", "_txt_nameplate_proxy", "logoOne", "img_logoTwo_ss", "_img_chevylogotype")
		};
		if((adm.cs.adSize == 'ws') || (adm.cs.adSize == 'mr')) {
			arr.push("txt_nameplate")
		};
		for(var k = 0; k < arr.length; k++) {
			Obj = adm.$(arr[k]);
			if(adm.ad.param("txt_firstframe") == "offer") {
				Obj.addClass("offer");
			} else {
				Obj.addClass("preroll");
			}
		}
	};
	
	
	if (adm.ad.param("txt_firstframe") == "offer") {
		if (adm.cs.adSize == "ws") {
			adm.$("txt_preroll").style.top = "130px";
		}
	}

	
	
	if(adm.ad.param("txt_eventlogo") == "on") {
		adm.cs.logosNum = "triple";
	} else {
		adm.cs.logosNum = "double";
	};
	arr = ["logoOne", "img_logoTwo_" + adm.cs.adSize, "img_logoThree_" + adm.cs.adSize, "_img_chevylogotype", "_img_findnewroads" ];
	if (adm.cs.adSize == "ws") {
		arr.push("txt_nameplate");
	}
	if (adm.cs.adSize == "ss") {
		arr.push("txt_nameplate", "logoOne", "img_logoTwo");
	}
	for(var l = 0; l < arr.length; l++) {
		Obj = adm.$(arr[l]);
		if(Obj) {
			Obj.addClass(adm.cs.logosNum);
		}
	};
	
});

adm.ad.on("complete", function() {

	
	if(document.body.className.indexOf("animation") < 0) {
		adm.$("template").addClass("static");
		adm.cs.isStatic = true;
	}
	
	
	adm.$("template").replaceClass("loading", "ready");
	
	
	
	
	adm.$('overlay').onmouseup = function(e) {
		if(adm.isRightClick(e)){ return false; }
		adm.htmlad.click('txt_link', adm.htmlad.param('txt_custom1'));
	};
	
	
	
	if(adm.$('template').hasClass('static')) {
		adm.$('legal_button').hide();
	} else if(adm.cs.isFallback && (adm.cs.isT3 || !adm.cs.isValidZip)) {
		adm.$('legal_button').hide();
	};
	
	
	if(!adm.ad.param("txt_link_2") || (adm.ad.param("txt_link_2") == "Optional CTA")) {
		adm.cs.cta2 = false;
		adm.$('cta2').hide();
		if (adm.cs.adSize == "ss") {
			adm.$('cta2').style.visibility = "hidden";
			adm.$('ctas').style.height = '20px';
			adm.$('txt_title').style.height = '50px';
		}
	} else if(adm.cs.adSize == 'mr') {
		if (adm.ad.param("txt_eventlogo") == "off") {
			adm.$('#template.mr #legal_button').style.top = '10px';
			adm.$('#template.mr #legal_button').style.left = '210px';
		} else {
		adm.$('#template.mr #legal_button').style.top = '158px';
		adm.$('#template.mr #legal_button').style.left = '210px';
		}
		if (adm.ad.param('txt_link_2')) {
			setTimeout(function() { adm.cs.cta2 = true}, 5000);
		}
	};
	
	
	
		var a = ["txt_preroll", "txt_title", "offer_subtitle",  "txt_nameplate"];
		if(adm.cs.adSize == 'ss') {
				a.push("_txt_nameplate_proxy")
			};
		if(adm.cs.adSize == 'lb') {
				a.push("txt_tagline")
			};
		if(adm.cs.adSize == 'sn' || adm.cs.adSize == 'sw' || adm.cs.adSize == 'ct') {
				a = ["txt_title", "txt_nameplate"];
			};
		if (adm.cs.isFallback == true) { 
			a = ["txt_title", "txt_tagline", "txt_nameplate"];
		};
		var aL = a.length;
		
		for(var i = 0; i < aL; i++) {
			var L = adm.ad.param(a[i]).length;
			var W = adm.$(a[i]).offsetWidth;
			var H = adm.$(a[i]).offsetHeight;
			var mP = Math.pow(L, 2);
			var A = W * H;
			
			var r1 = Math.ceil(Math.sqrt(L));
			var r2 = Math.ceil(Math.sqrt(A));
			var r3 = Math.floor(Math.sqrt(r1));
			
			var S = Math.round((r2 / r1) + (r3 / 2));
			
			var ls = Math.round("-" + (S / 3));
			
			
			
			
			if ( (typeof adm.ad. param(a[i]) === 'undefined') || (adm.ad. param(a[i]) === 'undefined') || (adm.ad. param(a[i]) === 'null') || (!adm.ad. param(a[i]))  ) {
			adm.$(a[i]).style.display='none'; 
			} else {
				var url = adm.ad.getiFontURL(a[i], {
				size: (S),
				linespacing: (ls),
				wordwrap: 1,
				});
			
			adm.$(a[i]).style.backgroundImage = 'url(' + url + ')';
			};
	};
	
	
	adm.$("legal_button").onmouseup = function(e) {
		adm.$("offer_legal_container").style.webkitAnimationName = 'slideUpLegal';
		adm.$("offer_legal_container").style.animationName = 'slideUpLegal';
		adm.$("offer_legal_container").style.webkitAnimationDuration = ".5s";
		adm.$("offer_legal_container").style.animationDuration = ".5s";
	};
	adm.$("close_button").onmouseup = function(e) {
		adm.$("offer_legal_container").style.webkitAnimationName = 'slideDownLegal';
		adm.$("offer_legal_container").style.animationName = 'slideDownLegal';
	};
	
	
	if(adm.cs.adSize == 'sw' || adm.cs.adSize == 'sn' || adm.cs.adSize == 'ct' || adm.cs.isFallback ) {
		adm.$('cta').innerHTML = adm.ad.param("txt_link");
	} else {
		adm.$('cta').innerHTML = adm.ad.param("txt_rollover");
		adm.$('cta2').innerHTML = adm.ad.param("txt_link_2");
		
		if (!adm.$('template').hasClass('static')) {
			if(adm.$('template').hasClass('preroll')) {
				setTimeout(function() {
					adm.$('cta').innerHTML = adm.ad.param("txt_link");
				}, 11000);
			} else {
				setTimeout(function() {
					adm.$('cta').innerHTML = adm.ad.param("txt_link");
				}, 4500);
			}
		}
	};
	
	
	adm.$('cta').onmouseover = function() {
		adm.$('cta2').style.visibility = "hidden";
		if(adm.cs.adSize == 'sw' || adm.cs.adSize == 'sn' || adm.cs.adSize == 'ct' || adm.cs.isFallback || adm.$('template').hasClass('static') ) {
			return;
		} else if (adm.cs.adSize == 'lb') {
			adm.$('cta').innerHTML = adm.ad.param("txt_rollover");
			adm.$('contentsliderlb').style.webkitAnimationName = ('ContentSlideIn2' + adm.cs.adSize);
			adm.$('contentsliderlb').style.animationName = ('ContentSlideIn2' + adm.cs.adSize);
			adm.$('legal_button').style.webkitAnimationName = ('slideOutDisclaimer1' + adm.cs.adSize);
			adm.$('legal_button').style.animationName = ('slideOutDisclaimer1' + adm.cs.adSize);
		} else  if (adm.cs.adSize == 'ss') {
				adm.$('_txt_nameplate_proxy').style.webkitAnimationName = ('slideInText' + adm.cs.adSize);
				adm.$('_txt_nameplate_proxy').style.animationName = ('slideInText' + adm.cs.adSize);
		}
		adm.$('cta').innerHTML = adm.ad.param("txt_rollover");
		adm.$('offer_container').style.webkitAnimationName = ('slideInOfferContainer' + adm.cs.adSize);
		adm.$('offer_container').style.animationName = ('slideInOfferContainer' + adm.cs.adSize);
		adm.$('slidebackground').style.webkitAnimationName = ('slideInPanel' + adm.cs.adSize);
		adm.$('slidebackground').style.animationName = ('slideInPanel' + adm.cs.adSize);
		adm.$('txt_preroll').style.opacity = 0;
	};

	adm.$("cta").onmouseleave = function() {
			adm.$('cta2').style.visibility = "visible";
		if(adm.cs.adSize == 'sw' || adm.cs.adSize == 'sn' || adm.cs.adSize == 'ct' || adm.cs.isFallback || adm.$('template').hasClass('static') ) {
			return;
		} else if (adm.cs.adSize == 'lb') {
			adm.$('cta').innerHTML = adm.ad.param("txt_link");
			adm.$('contentsliderlb').style.webkitAnimationName = ('ContentSlideOut' + adm.cs.adSize);
			adm.$('contentsliderlb').style.animationName = ('ContentSlideOut' + adm.cs.adSize);
			adm.$('legal_button').style.webkitAnimationName = ('slideBackDisclaimer1' + adm.cs.adSize);
			adm.$('legal_button').style.animationName = ('slideBackDisclaimer1' + adm.cs.adSize);
		} else if (adm.cs.adSize == 'ss') {
				adm.$('_txt_nameplate_proxy').style.webkitAnimationName = ('slideOutText' + adm.cs.adSize);
				adm.$('_txt_nameplate_proxy').style.animationName = ('slideOutText' + adm.cs.adSize);
		}
		adm.$('cta').innerHTML = adm.ad.param("txt_link");
		adm.$('offer_container').style.webkitAnimationName = ('slideOutOfferContainer' + adm.cs.adSize);
		adm.$('offer_container').style.animationName = ('slideOutOfferContainer' + adm.cs.adSize);
		adm.$('slidebackground').style.webkitAnimationName = ('slideOutPanel' + adm.cs.adSize);
		adm.$('slidebackground').style.animationName = ('slideOutPanel' + adm.cs.adSize);
		adm.$('txt_preroll').style.opacity = 1;
	};

	
	
	
	
	
	
	

	
	
	
	

	
	
	
	
	
	
	
	

	
	adm.$('cta').onmouseup = function(e) {
		if(adm.isRightClick(e)) {
			return false;
		}
		adm.htmlad.click('txt_link', adm.ad.param('txt_custom1'));
	};
	
	
	adm.$('cta2').onmouseup = function(e) {
		if(adm.isRightClick(e)) {
			return false;
		}
		adm.htmlad.click('txt_link', adm.ad.param('txt_custom2'));
	};

});

adm.ad.on( "click", function(e) {
	return (!adm.isRightClick(e));
} );





